{
	"name": "alfredoramos/seometadata",
	"type": "phpbb-extension",
	"description": "SEO Metadata extension for phpBB",
	"homepage": "https://github.com/AlfredoRamos/phpbb-ext-seo-metadata",
	"version": "2.0.0-dev",
	"time": "2025-11-01",
	"keywords": [
		"phpbb",
		"extension",
		"seo",
		"metadata",
		"open-graph",
		"twitter-cards",
		"json-ld",
		"schema-org",
		"microdata",
		"microformat",
		"rdf"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Alfredo Ramos",
			"email": "alfredo.ramos@proton.me",
			"homepage": "https://alfredoramos.mx",
			"role": "Lead Developer"
		}
	],
	"require": {
		"php": "^8.1.0",
		"composer/installers": "^2.2.0"
	},
	"require-dev": {
		"phing/phing": "^2.16.0",
		"pear/archive_tar": "^1.4.11"
	},
	"extra": {
		"display-name": "SEO Metadata",
		"soft-require": {
			"phpbb/phpbb": "~4.0.0"
		},
		"version-check": {
			"host": "alfredoramos.mx",
			"directory": "/phpbb-extensions/versions",
			"filename": "seo-metadata.json",
			"ssl": true
		}
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true
		}
	}
}